// This file is generated by the CPF and makes sure that the version info is baked into the binary.

#include <winresrc.h>

VS_VERSION_INFO VERSIONINFO
	FILEVERSION        @PACKAGE_VERSION_SHORT@	// We have to use the pseudo-release version here we get build errors.
	PRODUCTVERSION     @PACKAGE_VERSION_SHORT@	// Sadly this means that this version is wrong for internal builds.
	FILEFLAGSMASK      0x3fL
	FILEFLAGS          @FILE_FLAGS@
	FILEOS             VOS_NT_WINDOWS32
	FILETYPE           @FILE_TYPE@
	FILESUBTYPE        0x0L
	BEGIN
	BLOCK "StringFileInfo"
	BEGIN
	BLOCK "040904b0"
	BEGIN
	VALUE "CompanyName", "@OWNER@" "\0"
	VALUE "FileDescription", "@BRIEF_DESCRIPTION@" "\0"
	VALUE "FileVersion", "@PACKAGE_VERSION@" "\0"
	VALUE "InternalName", "@TARGET@" "\0"
	VALUE "LegalCopyright", "@COPY_RIGHT@" "\0"
	VALUE "OriginalFilename", "@FILE_NAME@" "\0"
	VALUE "ProductName", "@PACKAGE@"
	VALUE "ProductVersion", "@PACKAGE_VERSION@" "\0"
	END
	END
	BLOCK "VarFileInfo"
	BEGIN
	VALUE "Translation", 0x409, 1200
	END
	END
